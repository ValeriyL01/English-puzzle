{
  "name": "rss-puzzle",
  "version": "1.2.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve",
    "build-dev": "webpack",
    "build-prod": "webpack --node-env=production",
    "clear": "rd /s /q dist",
    "format": "prettier --write ./src",
    "ci:format": "prettier --check ./src",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "pre-commit": "npm run format && npm run ci:format",
    "pre-push": "npx validate-branch-name && npm run lint",
    "deploy": "npm run build-prod && npx gh-pages -d dist -e rss-puzzle"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "group-css-media-queries-loader": "^4.3.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^9.0.11",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^3.1.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged",
        "pre-push": "npm run eslint && npm run validate-branch-name"
      }
    },
    "lint-staged": {
      "*.{js,ts,jsx,json,css,scss,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "lint-staged": "^15.2.2"
  }
}
